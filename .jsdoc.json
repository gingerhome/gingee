    {
      "opts": {
        "template": "./node_modules/docdash",
        "destination": "./docs/api",
        "readme": "./docs/jsdoc-README.md",
        "recurse": true
      },
      "source": {
        "include": ["./modules"],
        "includePattern": ".js$",
        "excludePattern": "(node_modules/|docs)"
      },
      "plugins": [],
      "templates": {
        "cleverLinks": true,
        "monospaceLinks": true,
        "default": {
          "outputSourceFiles": false
        }
      }
    }
